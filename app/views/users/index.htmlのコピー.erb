<%= render 'books/list', book: @book, user: current_user %>

<h1>Users</h1>

<table>
  <tr>
  	<th>image</th><th>name</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
  	<th><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></th>
  	<th><%= user.name %></th>
    <th><%= link_to "Show", user_path(user) %></th>
  </tr>
  <% end %>
</table>

